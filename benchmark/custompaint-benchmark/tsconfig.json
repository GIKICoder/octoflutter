{
  "compilerOptions": {
    "target": "ES2015",
    "module": "es2015",
    "outDir": "./build/",
    "removeComments": true,
    "moduleResolution": "node",
    "baseUrl": "./",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["./src"]
}
